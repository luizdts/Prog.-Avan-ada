# include<stdlib.h>
int main(void){
  int mat[4];
  int *p, x;
  p = mat + 1;  // entrada válida! p recebe o valor do próximo elemento do vetor "mat".
  p = mat++; // declaração inválida
  p = ++mat; // declaração inválida
  x = (*mat)++; // declaração inválida, não realiza a soma
  printf("valor de x: %d", *p);
  return 0;
}
